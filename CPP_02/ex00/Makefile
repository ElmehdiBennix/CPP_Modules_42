EXE := 

CPP := clang++

CPPFLAGS := -Wall -Wextra -Werror

INC :=

SRC := 

OBJ :=

################################################################################ 

all: $(EXE)

$(EXE): $(OBJ)
	$(CPP) $(FLAGS) -o $@ $^

%.o: %.cpp $(INC)
	$(CPP) $(FLAGS) -c -o $@ $<

clean:
	rm -f $(OBJ)

fclean: clean

re: fclean all

################################################################################ 

.PHONY: all clean fclean re